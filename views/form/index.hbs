<div class="content-wrapper">
    <section class="content-header">
        <div class="callout callout-info">
            <h4>{{ content.title }}</h4>

            <p>{{{ content.content }}}</p>
            <p>Implementation Example: <a href="{{ content.url_implementation }}">Here</a></p>
            <p>What's include on this implementation example: {{ content.title_implementation }}</p>
        </div>
    </section>

    <section class="content">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">POST Example</h3>
            </div>
            <form role="form" method='POST'>
                <div class="box-body">

                    <h3>Your name is {{ name }}</h3>
                    <div class="form-group">
                        <label>Enter your name</label>
                        <input type="text" class="form-control" id="name" name="name" placeholder="Enter your name here...">
                    </div>
                </div>

                <div class="box-footer">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
    </section>
</div>